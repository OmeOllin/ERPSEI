                </svg>
                <div>
                    @Localizer["ProfilePicSuggestion"]
                </div>
            </div>
        </div>
        <div class="col-lg-6">
            <div class="form-floating mb-3">
                <input asp-for="Input.Username" class="form-control" placeholder="@Localizer["UsernamePlaceholder"]" disabled/>
                <label asp-for="Input.Username" class="form-label"></label>
            </div>
